public class CallAST {
  String funcname;
  ExpressionAST[] actuals;
  FUNCTION funcObj;

  public void Check() {
    IDENTIFIER F = ST.lookUpAll(funcname);

    if (F == Null) {
      System.err.println("Unknown function "+funcname);
    } else if (!F instanceof FUNCTION) {
      System.err.println(funcname+" is not a function");
    } else if (!F.formals.len == actuals.len) {
      System.err.println("wrong no. of params");
    } else {
      for (ExpressionAST k : actuals) {
       k.check();
       if (!F.equals(k.type())) {
         System.err.println("type of func param "+k+" incompatiable with declar
         ation");
       }
      }
      funcObj = F;
    }
  }
}
